    "@types/node": "^16.4.13",
    "@types/tmp": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "ajv": "^8.6.2",
    "ajv-formats": "^2.1.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "standard-version": "^9.3.1",
    "tmp": "^0.2.1",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
